{
  "name": "Autofill Bot",
  "version": "1.0",
  "description": "Autofill bot for quick checkout",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.streetwearofficial.com/*",
        "https://cncpts.com/*"
      ],
      "js": ["js/autofill.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "deafult_title": "Popup here"
  },
  "permissions": ["storage"]
}
